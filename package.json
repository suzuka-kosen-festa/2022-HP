{
  "name": "next-typescript-template",
  "version": "1.0.0",
  "description": "Template for personal Next projects",
  "keywords": [
    "next",
    "typescript",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/re-taro/next-typescript-template"
